<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">

   <context:annotation-config/>

    <bean id = "pointA" class = "org.ritubrata.com.Point" >   	
    	<property name="x" value = "0"></property>
    	<property name="y" value = "0"></property>	
   </bean>
   
   <bean id = "pointB" class = "org.ritubrata.com.Point"  >   	
    	<property name="x" value = "0"></property>
    	<property name="y" value = "20"></property>	
   </bean>
   
   <bean id = "pointC" class = "org.ritubrata.com.Point" >
   		<qualifier value = "quali"></qualifier>        
       	<property name="x" value = "20"></property>
    	<property name="y" value = "0"></property>	
   </bean>
   
   <bean id = "circle" class = "org.ritubrata.com.Circle" >   	
   </bean>
	
   </beans> 